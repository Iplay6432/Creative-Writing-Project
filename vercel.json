{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "static/**/*", "use": "@vercel/static" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/getChat", "dest": "/api/getChat.js" },
    { "src": "/api/getChatList", "dest": "/api/getChatList.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
